# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Database Configuration (optional - will use SQLite if not provided)
# DATABASE_URL=postgresql://username:password@localhost:5432/denguedb

# Security Keys (required for production)
SESSION_SECRET=your-session-secret-key-here
FLASK_SECRET_KEY=your-flask-secret-key-here

# Admin Configuration (optional - defaults provided)
# ADMIN_EMAIL=admin@dengue.com
# ADMIN_PASSWORD=admin123

# Flask Environment
FLASK_ENV=development
FLASK_DEBUG=True

# Email Configuration (optional)
# Weather API Configuration (required for live weather data)
OPENWEATHER_API_KEY=your-openweather-api-key-here

# Email Configuration (optional)
# MAIL_SERVER=smtp.gmail.com
# MAIL_PORT=587
# MAIL_USERNAME=your-email@gmail.com
# MAIL_PASSWORD=your-app-password
